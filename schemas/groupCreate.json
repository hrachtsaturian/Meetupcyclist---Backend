{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "description": {
        "type": "string",
        "maxLength": 90
      }
    },
    "additionalProperties": false,
    "required": [
      "name"
    ],
    "examples": [
      {
        "name": "Offroad Adventure",
        "description": "Explore rugged trails and enjoy breathtaking views."
      }
    ]
  }
  